{
  "name": "wp-graphql-relevanssi",
  "version": "0.0.1",
  "description": "A plugin to use Relevanssi on WP GraphQL",
  "main": "index.js",
  "scripts": {
    "init": "npm run standard-changelog:init && npm run changelog:init",
    "release": "npm run changelog && npm run standard-version",
    "changelog:init": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "standard-changelog:init": "standard-changelog --first-release",
    "standard-version": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-barbieri/wp-graphql-relevanssi.git"
  },
  "keywords": [
    "Relevanssi",
    "WP Graphql",
    "Wordpress",
    "plugin",
    "graphql",
    "API",
    "search"
  ],
  "author": "info@alessandrobarbieri.net",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/a-barbieri/wp-graphql-relevanssi/issues"
  },
  "homepage": "https://github.com/a-barbieri/wp-graphql-relevanssi#readme",
  "dependencies": {
    "conventional-changelog-cli": "^2.0.31",
    "prettier": "^1.19.1",
    "standard-changelog": "^2.0.21",
    "standard-version": "^8.0.1"
  }
}
